head
  link#bootstrap-css(href='http://maxcdn.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css' rel='stylesheet')
  script(src="https://kit.fontawesome.com/067b85bcc3.js" crossorigin="anonymous")
  script(src='http://maxcdn.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js')
  script(src='http://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js')
  link(rel='stylesheet', href='http://localhost:3000/css/page.css')
  script(src='http://localhost:3000/js/upload.js')
.container
  .row.header
    .col-sm
      a(href='/')
        img(src='http://localhost:3000/img/instagram.png' style='width: 50%')
    .col-sm
      form.search-form(action='/search' method='get')
        input(type='text' placeholder='검색')
        button(type='submit' style='width:0px; height:0px; display: none;')
    .col-sm.icon
      .filebox
        label(for='ex_file' style='width:30px;')
          i.fas.fa-upload
        input(type='file' id='ex_file' accept='image/*' onchange="imgupload(this.files);")
      i.fa.fa-home
      i.far.fa-sticky-note
      i.fab.fa-wpexplorer
      i.far.fa-user-circle
    .modal#myModal
      .modal-content
          input#contents(type='text' placeholder='문구 입력...')
          span.postbutton= '공유하기'
  block content